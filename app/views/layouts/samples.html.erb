</div><!-- #user_box -->				</div><!-- #topbar -->							<!-- STACKNAV (SIDEMENU) START -->				<% if !mobile_device? %>				<div id="sidebar" class="sidebar pjax_links">					<div class="cog">+</div>						<ul class="side_accordion" id="nav_side"> <!-- add class 'open_multiple' to change to from accordion to toggles -->						<% if current_user.is_admin? %>							<%= render 'layouts/side_admin' %>						<% elsif current_user.is_onsite? or current_user.is_doctor? %>							<%= render 'layouts/side_onsite' %>						<% elsif current_user.is_carrier? or current_user.is_cpld? %>							<%= render 'layouts/side_carrier' %>						<% end -%>					</ul>				</div>				<% end %>																<!-- STACKNAV (SIDEMENU) END -->									<!-- CENTER START -->								<div id="main_container" class="main_container container_16 clearfix">					<!-- PRINCIPAL MENU YIELD START -->					<div id="nav_top" class="dropdown_menu clearfix round_top">						<ul class="clearfix">							<% if current_user.is_admin? %>								<%= render 'layouts/menu_admin' %>							<% elsif current_user.is_onsite? or current_user.is_doctor? %>								<%= render 'layouts/menu_onsite' %>							<% elsif current_user.is_carrier? or current_user.is_cpld? %>								<%= render 'layouts/menu_carrier' %>							<% end -%>									</ul>														<div id="mobile_nav">							<div class="main"></div>						</div>										</div>											<!-- PRINCIPAL MENU YIELD END -->									<!-- MAIN CONTENT -->				 	<%= yield %>										<!-- MAIN CONTENT END -->				</div>						<!-- CENTER END -->								<!-- LOADING START -->					<div id="loading_overlay">					<div class="loading_message round_bottom">						<img src="/assets/interface/loading.gif" alt="loading" />					</div>				</div>				<!-- LOADING END -->								<!-- Google Analytics Code (Remove if not needed)				<script type="text/javascript">		  			var _gaq = _gaq || [];	  				_gaq.push(['_setAccount', 'UA-*******-*']