<?xml version="1.0" encoding="UTF-8"?>
<Response>
	<% if @order.urgency == "yes"
        	storou = "Stat"
        else
        	storou = "Routine"
     end %>
  <Say voice="woman">There is a new <%= storou %> order created at <%= @order.created_at.strftime("%l %M %p") %>. From <%= @order.origin_user.name %>. With tracking number is <%= @order.id %>. Please check your browser for further information.</Say>
  <Hangup/>
</Response>